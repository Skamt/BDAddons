{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"react-app",
		"eslint:recommended",
		"plugin:react/jsx-runtime",
		"plugin:react/recommended",
	],
	"rules": {
		"react/prop-types": [0],
		"react/display-name": [0],
		"react/style-prop-object": [1],
		"no-empty": [0],
		"no-multiple-empty-lines": [0]
	},
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"allowImportExportEverywhere": true
	},
	"settings": {
		"react": {
			"version": "18.2"
		}
	},
	"plugins": [
		"react"
	],
	"ignorePatterns":["**.css","**.json","**.png","**.md","**.gif","**.svg","**.TODO"],
	"globals": {
		"config": "readonly",
		"electron": "readonly",
		"process": "readonly",
		"module": "readonly",
		"require": "readonly",
		"BdApi": "readonly",
		"global": "readonly",
		"DiscordNative": "readonly"
	}
}