{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:react/jsx-runtime"
    ],
    "rules": {
        "no-multiple-empty-lines": [
            "error"
        ]
    },
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "react"
    ],
    "globals": {
        "useEffect": "readonly",
        "useState": "readonly",
        "Utils": "readonly",
        "Modules": "readonly",
        "require": "readonly",
        "BdApi": "readonly",
        "global": "readonly",
        "module": "readonly",
        "DiscordNative": "readonly"
    }
}